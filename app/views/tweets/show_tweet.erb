<% if logged_in? %>
  <%= @tweet.content %>
  <form method="post" action="/tweets/<%= @tweet.id %>/delete">
    <input id="hidden" type="hidden" name="_method" value="DELETE">
    <input type="submit" value="submit">
  </form>
  <a href="/tweets/<%= @tweet.id %>/edit">Edit Tweet</a>
<% else %>
  <a href="/login">Log In</a>
<% end %>
