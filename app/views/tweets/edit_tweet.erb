<% if logged_in? %>
  <%= @tweet.content %>
  <form method="post" action="/tweets/<%= @tweet.id %>">
    <input id="hidden" type="hidden" name="_method" value="patch">
    <input type="text" name="content" value="<%= @tweet.content %>">
    <input type="submit" value="Submit">
  </form>
<% else %>
  <a href="/login">Log In</a>
<% end %>
